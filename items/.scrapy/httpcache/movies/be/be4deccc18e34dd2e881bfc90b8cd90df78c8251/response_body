        }

        function consentKnown(e) {
            return e.detail.advertising === 'approved' || e.detail.advertising === 'denied';
        }

        function consentApproved(e) {
            return e.detail.advertising === 'approved';
        }

        function disableTracking() {
            if (window && window.branch) {
                window.branch.disableTracking(true);
            }
        }

        if (false) {
            document.addEventListener('gdpr-consent', function(e) {
                if (consentKnown(e)) {
                    initializeBranch(!consentApproved(e));
                    if (!consentApproved(e)) { disableTracking(); }
                }
            });
        } else {
            initializeBranch(false);
        }
